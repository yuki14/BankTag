<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/settings_add_bank_account_constraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/settings_add_bank_account_activity_settings_add_bank_account_constraint_layout_background_color">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/email_constraint_layout"
        android:layout_width="0dp"
        android:layout_height="@dimen/settings_add_bank_account_activity_email_constraint_layout_height"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="8dp">

        <View
            android:id="@+id/card_copy_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/settings_add_bank_account_activity_card_copy_constraint_layout_height"
            android:background="@color/settings_add_bank_account_activity_card_copy_constraint_layout_background_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="48dp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/row_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/settings_add_bank_account_activity_row_constraint_layout_height"
            android:layout_marginStart="@dimen/settings_add_bank_account_activity_row_constraint_layout_margin_start"
            android:layout_marginEnd="@dimen/settings_add_bank_account_activity_row_constraint_layout_margin_end"
            android:background="@color/settings_add_bank_account_activity_row_constraint_layout_background_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="0.91"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="48dp">

            <TextView
                android:id="@+id/add_iban_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:text="@string/settings_add_bank_account_activity_add_iban_text_view_text"
                android:textColor="@color/settings_add_bank_account_activity_add_iban_text_view_text_color"
                android:textSize="@dimen/settings_add_bank_account_activity_add_iban_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="15dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/header_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/settings_add_bank_account_activity_header_constraint_layout_height"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">

            <TextView
                android:id="@+id/iban_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/settings_add_bank_account_activity_iban_text_view_margin_start"
                android:gravity="start"
                android:text="@string/settings_add_bank_account_activity_iban_text_view_text"
                android:textColor="@color/settings_add_bank_account_activity_iban_text_view_text_color"
                android:textSize="@dimen/settings_add_bank_account_activity_iban_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="14dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/email_copy_constraint_layout"
        android:layout_width="0dp"
        android:layout_height="@dimen/settings_add_bank_account_activity_email_copy_constraint_layout_height"
        android:layout_marginTop="@dimen/settings_add_bank_account_activity_email_copy_constraint_layout_margin_top"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_constraint_layout"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="192dp">

        <View
            android:id="@+id/card_copy_two_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/settings_add_bank_account_activity_card_copy_two_constraint_layout_height"
            android:background="@color/settings_add_bank_account_activity_card_copy_two_constraint_layout_background_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="48dp"/>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/row_two_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/settings_add_bank_account_activity_row_two_constraint_layout_height"
            android:layout_marginEnd="@dimen/settings_add_bank_account_activity_row_two_constraint_layout_margin_end"
            android:layout_marginStart="@dimen/settings_add_bank_account_activity_row_two_constraint_layout_margin_start"
            android:background="@color/settings_add_bank_account_activity_row_two_constraint_layout_background_color"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="0.91"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="48dp">

            <TextView
                android:id="@+id/add_bic_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:text="@string/settings_add_bank_account_activity_add_bic_text_view_text"
                android:textColor="@color/settings_add_bank_account_activity_add_bic_text_view_text_color"
                android:textSize="@dimen/settings_add_bank_account_activity_add_bic_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="15dp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/header_two_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/settings_add_bank_account_activity_header_two_constraint_layout_height"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">

            <TextView
                android:id="@+id/bic_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/settings_add_bank_account_activity_bic_text_view_margin_start"
                android:gravity="start"
                android:text="@string/settings_add_bank_account_activity_bic_text_view_text"
                android:textColor="@color/settings_add_bank_account_activity_bic_text_view_text_color"
                android:textSize="@dimen/settings_add_bank_account_activity_bic_text_view_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="14dp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/navbar_image_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/settings_add_bank_account_activity_navbar_image_view_margin_bottom"
        android:scaleType="centerCrop"
        android:src="@drawable/navbar"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>